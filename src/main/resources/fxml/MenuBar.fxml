<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>

<MenuBar fx:id="menuBar" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.lwouis.f9.controllers.MenuBarController">
  <Menu mnemonicParsing="false" text="%menubar.item">
    <MenuItem mnemonicParsing="false" onAction="#chooseFile" text="%menubar.add" />
    <MenuItem mnemonicParsing="false" onAction="#removeSelected" text="%menubar.removeSelected" />
  </Menu>
</MenuBar>
