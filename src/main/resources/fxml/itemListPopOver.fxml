<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>

<VBox prefWidth="400.0" spacing="20.0" styleClass="vBox" xmlns="http://javafx.com/javafx/8.0.92" xmlns:fx="http://javafx.com/fxml/1">
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="25.0" />
  </padding>
  <JFXTextField fx:id="nameTextField" promptText="%popOver.name" style="-fx-label-float:true;" />
  <JFXTextField fx:id="pathTextField" promptText="%popOver.path" style="-fx-label-float:true;" />
  <JFXTextField fx:id="argumentsTextField" promptText="%popOver.arguments" style="-fx-label-float:true;" />
</VBox>
