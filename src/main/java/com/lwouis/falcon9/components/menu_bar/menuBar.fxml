<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>

<MenuBar fx:id="menuBar" xmlns="http://javafx.com/javafx/8.0.92" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.lwouis.falcon9.components.menu_bar.MenuBarController">
  <menus>
    <Menu mnemonicParsing="false" text="Item">
      <items>
        <MenuItem fx:id="addItem" mnemonicParsing="false" onAction="#chooseFile" text="Add" />
        <MenuItem fx:id="removeItem" mnemonicParsing="false" onAction="#removeSelected" text="Remove selected" />
      </items>
    </Menu>
  </menus>
</MenuBar>
